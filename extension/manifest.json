{
  "manifest_version": 2,
  "name": "Blind Store Project",
  "description": "An awesone extension to control your store",
  "version": "1.0",
  "author": "Aurelien L.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Blind Store Project"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab"]
}
